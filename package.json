{
  "name": "htdss",
  "version": "0.0.0",
  "description": "Hyundai Telecom Danji Support System",
  "author": {
    "name": "Hyundai Telecom",
    "github": "https://github.com/hyundaitel/HTDSS"
  },
  "homepage": "http://member-devel.hthomeservice.com:3005",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/hyundaitel/HTDSS.git"
  },
  "dependencies": {
    "@coreui/chartjs": "^2.0.0",
    "@coreui/coreui": "^3.3.0",
    "@coreui/icons": "2.0.0-beta.4",
    "@coreui/icons-react": "^1.0.2",
    "@coreui/react": "^3.3.0",
    "@coreui/react-chartjs": "^1.0.1",
    "@coreui/utils": "^1.3.1",
    "@sentry/browser": "^5.27.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.6.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "node-sass": "4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "xlsx": "^0.16.6"
  },
  "devDependencies": {
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-dev": "export PORT=3005 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject"
  },
  "bugs": {
    "url": "https://github.com/hyundaitel/HTDSS/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "no-console": "off",
      "linebreak-style": 0
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  }
}
